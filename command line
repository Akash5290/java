#include<stdlib.h>
#include<stdio.h>

int main(int argc,char*argv[])
{
	int sum=0,i,j;
	int n = atoi(argv[1]);
        printf("value of n:%d\n",n);
	int arr[n+1];
	for(i=2;i<=(n+1);i++)
	{
			arr[i]=atoi(argv[i]);
			printf("%d\n",arr[i]);
			sum=sum+arr[i];
	}		
	printf("value of argv:%d",sum);
	}
